{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "sld",
    "tlds"
  ],
  "properties": {
    "sld": {
      "$id": "#/properties/sld",
      "type": "string",
      "title": "The SLD Schema",
      "default": "",
      "examples": [
        "{{sld}}"
      ],
      "pattern": "^(.*)$"
    },
    "tlds": {
      "$id": "#/properties/tlds",
      "type": "array",
      "title": "The tld Schema",
      "examples": [
        "{tlds}}"
      ]
    }
  }
}