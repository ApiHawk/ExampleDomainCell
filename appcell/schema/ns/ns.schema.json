{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "nameservers"
  ],
  "properties": {
    "nameservers": {
      "$id": "#/properties/nameservers",
      "type": "array",
      "title": "The Nameservers Schema",
      "items": {
        "$id": "#/properties/nameservers/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "ns1.apihawk.com",
          "ns2.apihawk.com"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}