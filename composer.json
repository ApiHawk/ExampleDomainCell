{
    "name": "apihawk/microcell",
    "description": "MicroCell is a toolkit for writing AppCell plugins.",
    "keywords": [
        "apihawk",
        "appcell",
        "cell",
        "console",
        "worker"
    ],
    "homepage": "https://apihawk.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://www.apihawk.com/en/contacts/",
        "source": "https://github.com/ApiHawk/MicroCell-PHP"
    },
    "authors": [
        {
            "name": "Pavel Nikolov",
            "email": "p.nikolov@apihawk.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "enqueue/amqp-lib": "^0.10.1",
        "enqueue/enqueue": "^0.10.1",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/log": "^7.0",
        "laminas/laminas-config": "^3.3",
        "laminas/laminas-json": "^3.1",
        "laravel-zero/framework": "^7.0"
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "application"
    ]
}
